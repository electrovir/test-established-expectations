{
    "name": "test-established-expectations",
    "version": "0.0.0",
    "description": "For testing established expectations in a json file.",
    "keywords": [
        "test",
        "expect",
        "expectation",
        "comparison",
        "compare",
        "output"
    ],
    "homepage": "https://github.com/electrovir/test-established-expectations",
    "bugs": {
        "url": "https://github.com/electrovir/test-established-expectations/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/test-established-expectations"
    },
    "license": "MIT",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "compile": "rm -rf dist && virmator compile",
        "docs:update": "virmator code-in-markdown",
        "format": "virmator format",
        "publish": "virmator publish \"npm run compile && npm run test:all\"",
        "spellcheck": "virmator spellcheck",
        "test": "virmator test",
        "test:all": "npm run test:types && npm test && npm run spellcheck && npm run test:format && npm run test:docs",
        "test:docs": "virmator code-in-markdown check",
        "test:format": "virmator format check",
        "test:types": "tsc --noEmit"
    },
    "dependencies": {
        "@augment-vir/common": "^6.1.6",
        "@augment-vir/node-js": "^6.1.6",
        "chai": "^4.3.7"
    },
    "devDependencies": {
        "@augment-vir/chai": "^6.1.6",
        "@electrovir/nyc": "^15.1.0-fix0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.16",
        "cspell": "^6.17.0",
        "istanbul-smart-text-reporter": "^1.1.0",
        "markdown-code-example-inserter": "^0.1.13",
        "mocha": "^10.2.0",
        "mocha-spec-reporter-with-file-names": "^0.0.3",
        "prettier": "^2.8.1",
        "prettier-plugin-jsdoc": "^0.4.2",
        "prettier-plugin-multiline-arrays": "^1.1.2",
        "prettier-plugin-organize-imports": "^3.2.1",
        "prettier-plugin-packagejson": "^2.3.0",
        "prettier-plugin-sort-json": "^0.0.3",
        "prettier-plugin-toml": "^0.3.1",
        "ts-node": "^10.9.1",
        "type-fest": "^3.4.0",
        "typescript": "^4.9.4",
        "virmator": "^5.2.7"
    }
}
